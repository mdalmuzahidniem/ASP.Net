@model FirstApplication.Models.Entity.Product
@{
    ViewBag.Title = "Cart";
}

<h2>Cart</h2>
<table class="table table-hover">
    <tr>
        <td>Id</td>
        <td>Name</td>
        <td>Price</td>
        <td>Quantity</td>
        <td>Quality</td>
    </tr>
        <tr>
            <td>@Model.Id</td>
            <td>@Model.Name</td>
            <td>@Model.Price</td>
            <td>@Model.Quantity</td>
            <td>@Model.Description</td>
        </tr>

</table>

